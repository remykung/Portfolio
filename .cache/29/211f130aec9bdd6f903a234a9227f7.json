umn":30},"source":"Components/About.jsx","original":{"line":89,"column":2}},{"generated":{"line":82,"column":33},"source":"Components/About.jsx","original":{"line":91,"column":15},"name":"About"},{"generated":{"line":82,"column":38},"source":"Components/About.jsx","original":{"line":91,"column":20}}],"sources":{"Components/About.jsx":"/**\r\n * About component\r\n *\r\n * Space for you to describe more about yourself.\r\n */\r\n\r\nimport React from \"react\";\r\n\r\n/**\r\n * About background image\r\n *\r\n * Below is a sample image. Upload the image of your choice into the \"images\"\r\n * directory and import here for use. Then, set imageAltText to string that \r\n * represents what you see in that image.\r\n *\r\n * Need an image? Check out https://unsplash.com to download a image you\r\n * freely use on your site.\r\n */\r\nimport image from \"../images/motion-background.jpg\";\r\n\r\nconst imageAltText = \"purple and blue abstract background\";\r\n\r\n/**\r\n * Sort description that expands on your title on the Home component.\r\n */\r\nconst description =\r\n  \"Hello! I’m Remy, an IT and Data Science enthusiast committed to innovation and excellence. My career is a testament to continuous learning and impactful contributions across various tech domains. \";\r\n\r\n/**\r\n * List of some of skills or technologies you work on, are learning,\r\n * passionate about, or enjoy,\r\n */\r\nconst skillsList = [\r\n  \"Data Analysis & Warehousing\",\r\n  \"Machine Learning & AI\",\r\n  \"Database Management & SQL\",\r\n  \"Python, Java, JavaScript, TypeScript Programming\",\r\n  \"Front-End Technologies (HTML, CSS, React, Next.js)\",\r\n  \"Cloud Computing (AWS, Firebase)\",\r\n  \"Version Control (Git, GitHub, GitLab)\",\r\n  \"UX Prototyping with Figma\",\r\n  \"Business Analysis & IT Consulting\",\r\n  \"Data Visualization & Business Intelligence\",\r\n];\r\n\r\n\r\n/**\r\n * Use this to give more information about what you are passionate about,\r\n * how you best work, or even a quote. This will help someone learn more\r\n * about you on a professional level.\r\n */\r\nconst detailOrQuote =\r\n  \"I am passionate about solving problems in new creative ways to drive innovation. By leveraging my software experience I continually look for new and better ways to make tech accessible by all.\";\r\n\r\nconst About = () => {\r\n  return (\r\n    <section className=\"padding\" id=\"about\">\r\n      <img className=\"background\" src={image} alt={imageAltText} />\r\n      <div\r\n        style={{\r\n          backgroundColor: \"white\",\r\n          width: \"50%\",\r\n          padding: \"4rem\",\r\n          margin: \"3rem auto\",\r\n          textAlign: \"center\",\r\n        }}\r\n      >\r\n        <h2>About Myself</h2>\r\n        <p className=\"large\">{description}</p>\r\n        <hr />\r\n        <ul\r\n          style={{\r\n            textAlign: \"left\",\r\n            columns: 2,\r\n            fontSize: \"1.25rem\",\r\n            margin: \"2rem 3rem\",\r\n            gap: \"3rem\",\r\n          }}\r\n        >\r\n          {skillsList.map((skill) => (\r\n            <li key={skill}>{skill}</li>\r\n          ))}\r\n        </ul>\r\n        <hr />\r\n        <p style={{ padding: \"1rem 3rem 0\" }}>{detailOrQuote}</p>\r\n      </div>\r\n    </section>\r\n  );\r\n};\r\n\r\nexport default About;\r\n"},"lineCount":null}},"error":null,"hash":"c3ec0a3fb0dfd6aab0aec1cde813751d","cacheData":{"env":{}}}